<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Details</Details></title>

    
    

    <!-- JavaScript Bundle with Popper -->
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js" integrity="sha384-ka7Sk0Gln4gmtz2MlQnikT1wXgYsOg+OMhuP+IlRH9sENBO0LRn5q+8nbTov4+1p" crossorigin="anonymous"></script>

</head>
<body>
    


<div class="container-xl px-4 mt-4">
  <!-- Account page navigation-->
 
  <hr class="mt-0 mb-4">
  <%Details.forEach(details => {%>
      <div class="col-xl-8">
          <!-- Account details card-->
          <div class="card mb-4">
              
              <div class="card-body">
                  <form >
                      
                      <div class="mb-3 form-group">
                          <label class="small mb-1" >name : <%=details.name %> </label>
                      </div>
                     
                      


                    <div class="col-md-6">
                        <label class="small mb-1" >emailid: <%=details.emailId %></label>
                    </div>
                    <div class="col-md-6">
                        <label class="small mb-1" >emailid: <%=details.address %></label>
                    </div>
               

               
                    <div class="col-md-6">
                        <label class="small mb-1" >mobile: <%=details.mobile %></label>
                    </div>
                    <div class="col-md-6">
                        <label class="small mb-1" >mobile: <%=details.dateofbirth %></label>
                    </div>


                    <div class="col-md-6">
                        <label class="small mb-1" >fieldid: <%=details.fieldid %></label>
                    </div>

                    <div class="col-md-6">
                        <label class="small mb-1" >crop_cultivating: <%=details.crop_cultivating %></label>
                    </div>

                    <div class="col-md-6">
                        <label class="small mb-1" >type_of_soil: <%=details.type_of_soil %></label>
                    </div>

                        <div class="col-md-6">
                        <label class="small mb-1" >acre_of_land: <%=details.acre_of_land %></label>
                    </div>


                    <div<div class="col-md-6">
                        <label class="small mb-1" >years_of_experience: <%=details.years_of_experience %></label>

                    </div>

                   
                    
            </div>
                      <!-- Save changes button-->
                      
    



                      <a href="/del/<%= details._id %>" style="margin: 5%;">Delete</a>
                      <a href="/update/<%= details._id %>" style="margin: 5%;">Update</a>


                  </form>
              </div>
          </div>
      </div>

      <%})%>
  </div>
</div>

</body>
</html>
